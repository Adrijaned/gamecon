  <hr class="stin">

  <div class="hlavni"><!-- hlavní obsah -->
    <div class="obsah">
      {obsah}
      <div class="sloupce">
        <a class="blok upoutavka" href="">
          <img src="soubory/_placeholder/3.jpg">
          <div class="stredni"><div>Volný panel</div></div>
        </a>
        <a class="blok upoutavka" href="">
          <img src="soubory/_placeholder/4.jpg">
          <div class="stredni"><div>Volný panel</div></div>
        </a>
      </div>
    </div>
    <div class="blok sponzori">
      {sponzori}
    </div>
  </div>

</body>
</html>
