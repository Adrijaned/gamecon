<!-- begin: index -->
<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width">
  <base href="{base}">
  <link rel="shortcut icon" href="soubory/styl/favicon.ico">
  <link rel="stylesheet" type="text/css" href="{css}" media="screen,projection">
  <script src="{js}"></script>
  <!-- TODO title -->
  <title>GameCon</title>
</head>
<body>

  <div class="hlavicka">
    <a class="logo" href="">
      <img src="soubory/styl/logo.png" alt="GameCon"><br>
      Největší festival nepočítačových her
    </a>
    <div class="linfo">
      <a class="ifb" href="https://www.facebook.com/gamecon"></a>
      <b>17.–20. 7. 2014<br>Pardubice</b>
    </div>
    <div class="prihlasovani">
      <!-- begin:neprihlasen -->
      <div class="neprihlasen">
        <strong>Nejsete přihlášeni na web</strong><br>
        <span style="color: #bbb"><strong>Přihlásit</strong> / registrovat</span>
      </div>
      <!-- end:neprihlasen -->
      <!-- begin:prihlasen -->
        <img src="{u.avatar}">
        <!-- begin:admin -->

        <!-- end:admin -->
      <!-- end:prihlasen -->
    </div>
  </div>

